{
  "solargraph-docker": {
    "tcp": "localhost:17658",
    "root_uri_patterns": [
      "Gemfile",
      "Gemfile.lock",
      "Rakefile",
      "config.ru",
      ".ruby-version"
    ]
  },
  "volar-docker": {
    "cmd": "docker-compose exec -T volar npx vue-language-server --stdio",
    "root_uri_patterns": [
      "nuxt.config.ts",
      "package.json",
      "package-lock.json"
    ]
  }
}
